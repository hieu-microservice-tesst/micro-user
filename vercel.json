{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node",
        "config": {
        "buildCommand": "prisma generate && nest build",
        "includeFiles": ["prisma/schema.prisma", "prisma/generated/**/*"]
      }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": ["GET","POST","DELETE","PUT"]
      }
    ]
}